
@{
    ViewBag.Title = "SugarBaby";
    Layout = "~/Views/Shared/SBView.cshtml";
}

<h2>Sugar Baby Profile</h2>

<div class="row">
    <!-- left column -->
    <div class="col-md-6">
        <!-- general form elements -->
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">Basic Details</h3>
            </div>
            <!-- /.box-header -->
            <!-- form start -->
            <form role="form">
                <div class="box-body">
                    <div class="form-group">
                        <label for="exampleInputEmail1">User Name(Alias)</label>
                        <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Enter Alias">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword1">Profile Heading</label>
                        <input type="text" class="form-control" id="exampleInputPassword1" placeholder="Your Profile Heading">
                    </div>
                    <div class="form-group">
                        <label>Date Of Birth:</label>

                        <div class="input-group date">
                            <div class="input-group-addon">
                                <i class="fa fa-calendar"></i>
                            </div>
                            <input type="text" class="form-control pull-right" id="datepicker">
                        </div>
                        <!-- /.input group -->
                    </div>
                    <div class="form-group">
                        <label>Seeking</label>
                        <select class="form-control select2" style="width: 100%;">
                            <option selected="selected">Men</option>
                            <option>Women</option>
                            <option>Both</option>
                        </select>
                    </div>
                    
                    
                </div>
                <!-- /.box-body -->

                <div class="box-footer">
                    <button type="submit" class="btn btn-primary">Submit</button>
                </div>
            </form>
        </div>
        <!-- /.box -->
